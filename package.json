{
  "name": "photography",
  "version": "1.0.0",
  "description": "Photography by Sebastian Sandqvist",
  "main": "server.js",
  "engines": {
    "node": ">=4.1.0"
  },
  "scripts": {
    "lint": "jscs server.js --config=./config/jscs.json && eslint server.js -c ./config/eslint.json",
    "prelint": "jscs -e app/**/*.js --config=./config/jscs.json && eslint app/**/*.js -c ./config/eslint.json",
    "start": "NODE_ENV=production node server",
    "dev": "node server",
    "predev": "npm run build",
    "pack": "webpack --progress --watch",
    "build": "NODE_ENV=production webpack --progress -p --config webpack.production.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastiansandqvist/photography.git"
  },
  "keywords": [
    "photography"
  ],
  "author": "sebastian sandqvist",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sebastiansandqvist/photography/issues"
  },
  "homepage": "https://github.com/sebastiansandqvist/photography",
  "devDependencies": {
    "@seabass/eslint-config": "^1.0.9",
    "babel-core": "^5.8.24",
    "babel-loader": "^5.3.2",
    "clean-webpack-plugin": "^0.1.3",
    "eslint": "^1.4.3",
    "jscs": "^2.1.1",
    "webpack": "^1.12.2",
    "webpack-split-by-path": "0.0.6"
  },
  "dependencies": {
    "compression": "^1.5.2",
    "express": "^4.13.3",
    "radium": "^0.14.1",
    "react": "^0.13.3"
  }
}
